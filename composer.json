{
    "name": "your1/qdrant",
    "description": "PHP Client for Qdrant",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": "^2.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mockery/mockery": "*",
        "phpunit/php-code-coverage": "*"
    },
    "autoload": {
        "psr-4": {
            "Qdrant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qdrant\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Haydar Kulekci",
            "email": "haydarkulekci@gmail.com"
        },
        {
            "name": "Youri van den Bogert",
            "email": "yvdbogert@archixl.nl"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --display-warnings --display-deprecations",
        "coverage": "vendor/bin/phpunit --display-warnings --display-deprecations --coverage-html coverage/"
    }
}
